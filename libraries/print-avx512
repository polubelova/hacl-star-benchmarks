#!/bin/bash

echo $'CHACHA20' > results-avx512.txt && \
cd ./../kbench9000-user-avx512/chacha && \
    make clean && CC=gcc-9 CFLAGS=-w make run 2>> ../../libraries/results-avx512.txt && \
echo $'\n\nPOLY1305' >> ../../libraries/results-avx512.txt && \
cd ../poly1305 && \
    make clean && CC=gcc-9 CFLAGS=-w make run 2>> ../../libraries/results-avx512.txt && \
echo $'\n\nSHA512' >> ../../libraries/results-avx512.txt && \
cd ../sha512 && \
    make clean && CC=gcc-9 CFLAGS=-w make run 2>> ../../libraries/results-avx512.txt && \

echo $'\n\nBLAKE2B' >> ../../libraries/results-avx512.txt && \
cd ../../kbench9000-user-noavx512/blake2b && \
    make clean && CC=gcc-9 CFLAGS=-w make run 2>> ../../libraries/results-avx512.txt && \
echo $'\n\nBLAKE2S' >> ../../libraries/results-avx512.txt && \
cd ../blake2s && \
    make clean && CC=gcc-9 CFLAGS=-w make run 2>> ../../libraries/results-avx512.txt && \
echo $'\n\nSHA256' >> ../../libraries/results-avx512.txt && \
cd ../sha256 && \
    make clean && CC=gcc-9 CFLAGS=-w make run 2>> ../../libraries/results-avx512.txt 
